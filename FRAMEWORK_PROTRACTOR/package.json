{
  "name": "productcreation-cucumber",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build-e2e": "tsc --project tsconfig-e2e.json",
    "clean": "rimraf typeScript/",
    "clean-build": "npm run clean && npm run build",
    "clean-build-e2e": "npm run clean && npm run build-e2e",
    "webdriver-update": "webdriver-manager update --gecko false --ignore_ssl",
    "webdriver-start": "webdriver-manager start --detach",
    "webdriver-upstart": "npm run webdriver-update && npm run webdriver-start",
    "test-qa-ci": "npm run test-qa-grid",
    "test-dev-ci": "npm run test-dev-grid",
    "test-qa": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=QA  --run-mode=H --use-grid=false",
    "test-dev": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=INT --run-mode=H --use-grid=false",
    "test-local": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=LOCAL --run-mode=H --use-grid=false",
    "test-qa-nh": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=QA --run-mode=NH --use-grid=false",
    "test-dev-nh": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=INT --run-mode=NH --use-grid=false",
    "test-local-nh": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=LOCAL --run-mode=H --use-grid=false",
    "test-qa-grid": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=QA  --run-mode=H --use-grid=true",
    "test-dev-grid": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=INT --run-mode=H --use-grid=true",
    "test-local-grid": "npm run clean-build && protractor typeScript/config/config.js --disableChecks --env=LOCAL --run-mode=H --use-grid=true",
    "test-qa-e2e": "npm run clean-build-e2e && protractor typeScript/config/config.js --disableChecks --env=QA  --run-mode=H --use-grid=true"
  },
  "dependencies": {
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/chai-string": "1.4.2",
    "@types/crypto-js": "3.1.47",
    "@types/cucumber": "6.0.1",
    "@types/fs-extra": "9.0.1",
    "@types/mssql": "6.0.4",
    "@types/node": "14.10.1",
    "@types/selenium-webdriver": "4.0.9",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "crypto-js": "4.0.0",
    "cucumber": "6.0.5",
    "cucumber-html-reporter": "5.2.0",
    "fs-extra": "9.0.1",
    "moment": "2.28.0",
    "mssql": "6.2.1",
    "protractor": "7.0.0",
    "protractor-cucumber-framework": "6.2.1",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.2",
    "webdriver-manager": "12.1.7"
  }
}
