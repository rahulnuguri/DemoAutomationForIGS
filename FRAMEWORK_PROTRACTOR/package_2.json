{
  "name": "productcreation",
  "version": "2.1.9",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build-dcos": "npm run env -s && ng build --configuration=dcos",
    "build-dev": "npm run env -s && ng build --configuration=dev",
    "build-qa": "npm run env -s && ng build --configuration=qa",
    "build-prod": "npm run env -s && ng build --configuration=prod",
    "build": "npm run env -s && ng build",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/core": "^9.1.6",
    "@angular/flex-layout": "^9.0.0-beta.30",
    "@angular/forms": "^9.1.6",
    "@angular/http": "^7.2.16",
    "@angular/localize": "^9.1.6",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@angular/router": "^9.1.6",
    "@angular/service-worker": "^9.1.6",
    "@ng-bootstrap/ng-bootstrap": "^4.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-user-idle": "^2.2.2",
    "bootstrap": "^4.5.0",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "debug": "^3.2.6",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "material-design-icons-iconfont": "^3.0.3",
    "rxjs": "^6.5.5",
    "tslib": "^1.12.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.5",
    "@angular/cli": "^9.1.5",
    "@angular/compiler-cli": "^9.1.6",
    "@angular/language-service": "^9.1.6",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^2.1.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.150",
    "@types/node": "^8.10.60",
    "codelyzer": "^4.5.0",
    "hads": "^1.7.3",
    "htmlhint": "^0.10.3",
    "https-proxy-agent": "^2.2.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "^5.4.2",
    "puppeteer": "^1.20.0",
    "stylelint": "~9.2.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.1.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~3.8.3"
  }
}
